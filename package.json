		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.4.3",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vite-plugin-dts": "^1.7.1"
	},
	"peerDependencies": {
		"react-router-dom": "^6.4.3"

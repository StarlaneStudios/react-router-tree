{
	"name": "react-router-tree",
	"description": "Implement Next.js style routing in any Vite or Webpack based react application",
	"version": "1.0.4",
	"type": "module",
	"files": [
		"dist"
	],
	"license": "MIT",
	"main": "./dist/react-router-tree.umd.cjs",
	"module": "./dist/react-router-tree.js",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/react-router-tree.js",
			"require": "./dist/react-router-tree.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepublishOnly": "pnpm build",
		"test": "vitest",
		"test:ui": "vitest --ui"
	},
	"devDependencies": {
		"@types/node": "^18.7.18",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"@vitejs/plugin-react": "^2.1.0",
		"@vitest/ui": "^0.23.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "6.4.1",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.3",
		"vite-plugin-dts": "^1.5.0",
		"vitest": "^0.23.4"
	},
	"peerDependencies": {
		"react-router-dom": "6.4.1"
	}
}
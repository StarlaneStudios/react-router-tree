{
	"name": "react-router-tree",
	"description": "Implement Next.js style routing in any Vite or Webpack based react application",
	"version": "1.0.6",
	"type": "module",
	"files": [
		"dist"
	],
	"license": "MIT",
	"main": "./dist/react-router-tree.umd.cjs",
